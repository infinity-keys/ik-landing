{
  "name": "infinity-keys-landing",
  "private": true,
  "scripts": {
    "dev": "yarn generate:watch & next dev",
    "build": "yarn generate:once && next build",
    "start": "next start",
    "lint": "tsc && next lint",
    "generate:once": "yarn run typechain && graphql-codegen",
    "generate:watch": "yarn run typechain && graphql-codegen --watch",
    "fmt": "eslint . --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless --browser chrome",
    "cypress:ci": "yarn build && start-server-and-test start http://localhost:3000 cypress:run",
    "typechain": "typechain --target ethers-v5 \"pages/future/dev/**/*.json\""
  },
