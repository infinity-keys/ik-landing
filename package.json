    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5",
    "@walletconnect/web3-provider": "^1.7.8",
    "@xstate/react": "^3.0.0",
    "boring-avatars": "^1.7.0",
