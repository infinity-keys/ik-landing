{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "yarn run typechain && yarn rw dev",
    "typechain": "typechain --target ethers-v5 \"nfts/contracts/**/*.json\" --out-dir web/src/lib/generated/ethers-contract"
  },
  "devDependencies": {
    "@redwoodjs/core": "^3.2.0",
    "@typechain/ethers-v5": "^10.1.0",
    "typechain": "^8.1.0"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.3"
}
