{
  "name": "infinity-keys-landing",
  "private": true,
  "scripts": {
    "dev": "yarn generate:watch & sleep 5 && next dev",
    "build": "yarn generate:once && next build",
    "next:build": "next build",
    "start": "next start",
    "lint": "tsc && next lint",
    "generate:once": "yarn run typechain && graphql-codegen",
    "generate:watch": "yarn run typechain && graphql-codegen --watch",
    "fmt": "eslint . --fix",
    "cypress:open": "NEXT_PUBLIC_IS_CYPRESS=true start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "cypress:run": "cypress run --headless --browser chrome",
    "cypress:ci": "yarn build && start-server-and-test start http://localhost:3000 cypress:run",
    "typechain": "typechain --target ethers-v5 \"nfts/contracts/**/*.json\" --out-dir lib/generated/ethers-contract",
    "analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.9.1",
    "@coinbase/wallet-sdk": "^3.5.0",
    "@formspree/react": "^2.2.4",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-graphql-request": "^4.5.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5",
    "@walletconnect/web3-provider": "^1.8.0",
    "@xstate/react": "^3.0.1",
    "boring-avatars": "^1.7.0",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.1.1",
    "ethers": "^5.6.5",
    "fortmatic": "^2.3.0",
    "graphql": "^16.6.0",
    "graphql-request": "^4.2.0",
    "jose": "^4",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "next": "12.2.5",
    "nprogress": "^0.2.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.34.2",
    "react-individual-character-input-boxes": "^1.1.0",
    "react-markdown": "^8.0.3",
    "uuid": "^8.3.2",
    "web3modal": "^1.9.8",
    "xstate": "^4.32.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@next/bundle-analyzer": "^12.2.4",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18",
    "@types/uuid": "^8.3.4",
    "@xstate/inspect": "^0.7.0",
    "cypress": "^10.6.0",
    "eslint": "^8",
    "eslint-config-next": "^12",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typechain": "^8.1.0",
    "typescript": "^4",
    "yarn-audit-fix": "^9.3.5"
  },
