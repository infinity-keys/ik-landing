{
  "name": "@infinity-keys/contracts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "typechain --target ethers-v5 src/contracts/**/*.json --out-dir src/generated/ && tsc && vite build",
    "build-dev": "typechain --target ethers-v5 src/contracts/**/*.json --out-dir src/generated/ && tsc && cross-env NODE_ENV=development vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.11.9",
    "cross-env": "^7.0.3",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tslib": "^2.4.1",
    "typescript": "^4.6.4",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@infinity-keys/constants": "workspace:^",
    "@typechain/ethers-v5": "^10.1.0",
    "ethers": "^5.7.2",
    "typechain": "^8.1.0"
  }
}
